{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "singer_formatter": {
      "format": "%(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "singer_formatter",
      "stream": "ext://sys.stderr"
    }
  },
  "root": {
    "handlers": [
      "console"
    ],
    "level": "INFO"
  }
}